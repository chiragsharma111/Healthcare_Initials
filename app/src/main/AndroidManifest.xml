<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.chrg.smarthealthcareassistant">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:targetApi="m">
        <activity android:name=".view_user_data"></activity>
        <activity android:name=".edit_user_data" />
        <activity android:name=".UserData" />
        <activity android:name=".RegisterPage" />
        <activity android:name=".LoginPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FeedbackConfirmed" />
        <activity android:name=".Feedback" />
        <activity android:name=".HistoryAppointments" />
        <activity android:name=".ConfirmationPage" />
        <activity android:name=".pay_and_book" />
        <activity android:name=".CategoryPage2" />
        <activity android:name=".CategoryPage" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>